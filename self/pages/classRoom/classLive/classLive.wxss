.qnaire {
	width: 100%;
	height: 100%;
	position: fixed;
	left: 0;
	top: 0;
	/*z-index: 999;*/
	background-color: #fff;
	overflow-y: scroll;
}
.naire-title {
	height: 90rpx;
	line-height: 90rpx;
	color: #0078d7;
	font-size: 30rpx;
	padding-left: 30rpx;
	position: relative;
}
.naire-close {
	width:26rpx;
	height:26rpx;
	line-height: 26rpx;
	position:absolute;
	padding:10rpx;
	top: 22rpx;
	right: 22rpx;

}
.naire-point {
	height: 40rpx;
	line-height: 40rpx;
	font-size: 22rpx;
	color: #ff665d;
	padding-left: 30rpx;
	background-color: #ffe7e6;
	border-top: #e6e6e6;
}
.naire-single, .naire-multiple, .naire-area {
	padding-left: 30rpx;
	padding-right: 20rpx;
	margin-bottom: 30rpx;
}

.single-title {
	line-height: 38rpx;
	font-size: 26rpx;
	color: #666; 
	white-space: normal;
}
.single-item {
	position: relative;
	margin-top: 26rpx;
}
.single-choose {
	width: 44rpx;
	height: 44rpx;
	position: absolute;
}
.single-text {
	font-size: 26rpx;
	color: #666;
	line-height: 44rpx;
	white-space: normal;
	padding-left: 56rpx;
	word-break: break-all;
}

.naire-submit {
	width: 100%;
	height: 120rpx;
	line-height: 120rpx;
	position: fixed;
	z-index: 1000;
	background-color: #fff;
	text-align: center;
	bottom: 0;
	left: 0;
}
.submit-button {
	width: 370rpx;
	height: 80rpx;
	line-height: 80rpx;
	text-align: center;
	font-size: 26rpx;
	margin: 20rpx auto;
	background-color: #0078d7;
	border-radius: 80rpx;
	color: #fff;
}
.qnArea {
	height: 120rpx;
	width: 650rpx;
	font-size: 24rpx;
	min-height: 120rpx;
	border: 2rpx solid #cecece;
	margin-top: 20rpx;
	padding:6rpx 10rpx;
	word-break: break-all;
}

.result-text {
	font-size: 26rpx;
	color: #666;
	line-height: 44rpx;
	white-space: normal;
	display: flex;
}
.resultNum {
	display: block;
    width: 48rpx;
    text-align: center;
    line-height: 40rpx;
    overflow: hidden;
}
.resultPro {
	flex: 1.0;
	padding-top: 4rpx;
}
.proOut {
 	border: 1px solid #dedddd;
    border-radius: 16rpx;
    background-clip: padding-box;
    background-color: #f4f4f4;
    height: 16rpx;
    font-size: 0;
    line-height: 16rpx;
    margin-top: 10rpx;
    position: relative;
}
.proIn {
	position: absolute;
    height: 16rpx;
    left: 0;
    font-size: 0;
    line-height: 16rpx;
    border-radius: 16rpx;
    background-color: #0078d7;
}
.resultTotal {
	width: 80rpx;
    display: block;
    text-align: right;
    line-height: 20px;
    overflow: hidden;
}
.areaResult {
	font-size: 24rpx;
	color: #0078d7;
	margin-left: 32rpx;
	margin-top: 10rpx;
}
.showTrue {
	height: 28rpx;
	line-height: 28rpx;
	color: #fff;
    font-size: 18rpx;
    border-radius: 6rpx;
    margin-top: 6rpx;
    display: block;
	width: 50rpx;
	text-align: center;
}
.showTrueBg {
	background-color: #0078d7;
}