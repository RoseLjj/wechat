/* pages/videodemo/videodemo.wxss */
.backView {
  width: 100%;
  height: 64px;
  background-color: #1ADAAF;
  display: flex;
  justify-content: center;
  align-items: center;
}

.backImage {
  width: 32px;
  height: 32px;
  position: absolute;
  margin-top: 24px;
  margin-left: 16px;
}

.titleLabel {
  width: 300rpx;
  color: white;
  font-family: PingFangSC-Regular;
  font-size: 16px;
  line-height: 16px;
  margin-top: 20px;
  overflow:hidden; 
  text-overflow:ellipsis;
  white-space:nowrap;  
  text-align:center;
}

.videoView {
	width: 100%; 
	position: relative;
	background-color: #1b1b23;
	overflow:hidden;
}
.videoView .myVideo {
	width: 100%;
	height: 100%;
}
.videoView .onlyAudio{
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	z-index: 99;
}
.videoView .overHand {
	width: 100%;
	height: 100%;
	z-index: 998;
	position: absolute;
	top: 0;
	left: 0;
}
.videoView .vTips {
	width: 100%;
	height: 80rpx;
	line-height: 80rpx;
	font-size: 22rpx;
	color: rgba(255, 255, 255, 1);
	background-color: rgba(0, 0, 0, 0.6);
	font-size: 22rpx;
	position: absolute;
	bottom: 0;
	left: 0;
	z-index: 999;
}

.videoView .tipsLeft {
	float: left;
	margin-left: 20rpx;
}
.videoView .tipsLeft .playStatus {
	height: 40rpx;
	line-height: 50rpx;
}

.videoView .tipsLeft .statusIcon{
	width: 16rpx;
	height: 16rpx;
	background-color: #ef5045;
	border-radius: 100%;
	float: left;
	margin-top: 22rpx;
	margin-right: 10rpx;
}
.videoView .tipsLeft .statusText {
	margin-top: 16rpx;
	float: left;
}

.videoView .tipsLeft .playNum {
	height: 40rpx;
	line-height: 30rpx;
}
.videoView .tipsLeft .numIcon {
	width: 16rpx;
	height: 18rpx;
	float: left;
	margin-top: 8rpx;
	margin-right: 10rpx;
	/*background: url("http://192.168.0.166/images/icons.png") no-repeat;
	background-position: 1% 1%;
	background-size: 210px 150px;*/
}
.videoView .tipsLeft .numText {
	margin-top: 4rpx;
	float: left;
}


.videoView .tipsRight {
	float: right;
}
.videoView .video-control, .full-screen {
	width: 40rpx;
	height: 40rpx;
	padding: 10rpx;
	margin-top: 10rpx;
	display: inline-block;
}
.videoView .video-control {
	margin-right: 30rpx;
}
.videoView .full-screen {
	margin-right: 20rpx;
}
.notStart {
	width: 350rpx;
	height: 208rpx;
	margin: 0 auto;
	display: block;
}
.endVideo {
	width: 100%;
	text-align: center;
	font-size: 34rpx;
	color: #fff;
}
.endVideoSmall {
	font-size: 28rpx;
}



.swiper-title {
	width: 100%;
	height: 78rpx;
	line-height: 78rpx;
	border-bottom: 1px solid #e6e6e6;
	display: flex;
}
.swiper-title .swiper-demo {
	flex: 1;
	text-align: center;
	font-size: 30rpx;
	color: #666666;
}
.swiper-title .swiper-in {
	height: 78rpx;
	width: 78rpx;
	margin: 0 auto;
}
.swiper-title .choose {
	color: #0078d7;
	height: 74rpx;
	border-bottom: 2px solid #0078d7;
}







.swiper-main {
	width: 100%;
	position:relative;
}
.swiper-item {
	width: 100%;
	height: 100%;
	position: relative;
	/*overflow-y: scroll;*/
}
.canvas {
	width: 100%;
}
.noppt {
	width: 100%;
	height: 100%;
	text-align: center;
}
.noppt-img {
	width: 160rpx;
	height: 90rpx;
	margin: 0 auto;
}
.noppt-img2 {
	width: 160rpx;
	height: 90rpx;
}
.noppt-text {
	color: #b7b7b7;
	font-size: 24rpx;
	height: 40rpx;
	margin-top: -40rpx;
	display: block;
}
