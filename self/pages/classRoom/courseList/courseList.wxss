/* pages/classRoom/courseList/courseList.wxss */
@import '../../../common/common.wxss';
@import '../../../components/component.wxss';

.backgroundImage {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}

.course-list-box {
  height: 894rpx;
}

.subject-list view {
  display: inline-block;
}

.subject-list view.left-tips {
  left: 20rpx;
}

.subject-list view.right-tips {
  right: 0;
}

.list-tips {
  padding: 20rpx;
  height: 56rpx;
  line-height: 56rpx;
  margin-top: 20rpx;
  font-size: 22rpx;
  position: absolute;
}

.subject-list .list-tips::after {
  content: "";
  position: absolute;
  width: 0;
  height: 0;
  border-style: solid;
  top: 0px;
}

.subject-list view.left-tips::after {
  border-width: 28rpx 0 28rpx 20rpx;
  border-color: transparent transparent transparent #ff7474;
  right: -19rpx;
}

.subject-list .right-tips::after {
  border-width: 28rpx 20rpx 28rpx 0;
  border-color: transparent #f8cd4b transparent transparent;
  left: -19rpx;
}

/* 今日课程 */

.continue-questions {
  margin-left: 20rpx;
  margin-right: 20rpx;
  height: 88rpx;
  color: #bc849b;
