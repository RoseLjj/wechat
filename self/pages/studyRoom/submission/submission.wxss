/* pages/studyRoom/submission/submission.wxss */
@import '/common/common.wxss';
@import '../correctRate/correctRate.wxss';

.correct-box>view {
  padding-bottom: 30rpx;
}

.number-list {
  padding: 20rpx 40rpx;
}

.corate {
  margin: 50rpx 0;
}

.tips-title {
  padding: 0 80rpx;
  height: 48rpx;
  line-height: 48rpx;
  padding-top: 12rpx;
  display: flex;
  font-size: 24rpx;
  flex-wrap: nowrap;
  justify-content: space-between;
  background-color: rgba(255, 255, 255, 0.50);
}

.tips-title .tips_one view {
  display: inline-block;
}

.tips-title .tips_one view:nth-of-type(1) {
  width: 28rpx;
  height: 28rpx;
}

.tips-title .tips_one view:nth-of-type(2) {
  vertical-align: top;
  margin-left: 16rpx;
  position: relative;
  top: -4rpx;
}

.tips-title .tips-weida view:nth-of-type(1) {
  background: rgba(255, 255, 255, 0.50);
}

.tips-subject {
  height: 86rpx;
  background: rgba(255, 255, 255, 0.50);
  margin-top: 1rpx;
  line-height: 86rpx;
  font-size: 36rpx;
  color: #08b58f;
  position: relative;
}

.right-list {
  height: 350rpx;
}

.tips-subject .leftJiao {
  display: block;
  width: 0;
  height: 0;
  border-width: 20rpx 20rpx 20rpx 0;
  border-style: solid;
  border-color: transparent #08b58f transparent transparent; /*透明 黄 透明 透明 */
  position: absolute;
  top: 20rpx;
  left: 20rpx;
}

.tips-subject .rightJiao {
  display: block;
  width: 0;
  height: 0;
  border-width: 20rpx 0 20rpx 20rpx;
  border-style: solid;
  border-color: transparent transparent transparent #08b58f; /*透明 黄 透明 透明 */
  position: absolute;
  top: 20rpx;
  right: 20rpx;
}

.zonghe {
  width: 192rpx;
  height: 52rpx;
  line-height: 52rpx;
  display: inline-block;
  background-color: #f8cd4b;
  font-size: 28rpx;
  color: #fff;
  margin-bottom: 30rpx;
}

.useTime view {
  display: inline-block;
}

.chaoguo .btn-apply, .chaoguo .btn-record {
  border-radius: 0;
  width: 240rpx;
  height: 76rpx;
  background: #1adaaf;
  font-size: 32rpx;
  color: #fff;
}

.chaoguo .btn-al-apply {
  background: #c6c6c6;
  border-radius: 0;
  width: 240rpx;
  height: 76rpx;
  font-size: 32rpx;
  color: #fff;
}

.chaoguo .btn-zonghe {
  background: #f8cd4b;
  border-radius: 0;
  width: 240rpx;
  height: 76rpx;
  font-size: 32rpx;
  color: #fff;
  margin-top: 50rpx;
}

.margin-top50{
  margin-top: 50rpx;
}
